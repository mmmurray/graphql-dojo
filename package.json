{
  "name": "graphql-dojo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "glamor": "^2.20.40",
    "glamorous": "^4.12.5",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.0",
    "material-ui": "^0.20.0",
    "node-fetch": "^2.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build:server": "babel src/server --out-dir lib --copy-files",
    "prettier": "prettier --write '{src/**/*,test/**/*,*}.{js,json}'",
    "start": "npm run build:server && run-p start:*",
    "start:app": "react-scripts start",
    "start:server": "nodemon -e js,graphql --watch lib/ ./lib/server.js",
    "start:server-watch": "npm run build:server -- --watch --skip-initial-build",
    "start:mock-tfgm": "node ./mock-apis/tfgm/server.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1"
  }
}
